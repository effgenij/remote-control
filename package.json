{
    "name": "ws_task",
    "version": "1.0.0",
    "description": "Template for the WebSocket remote control task",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "npm run build && npm run prod",
        "start:dev": "nodemon ./index.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "prod": "cross-env NODE_ENV=production node dist/index.js",
        "pretty": "prettier './**/*.ts'",
        "pretty:fix": "prettier --write './**/*.ts'",
        "lint": "eslint './**/*.ts'",
        "lint:fix": "eslint --fix './**/*.ts'",
        "style:fix": "npm run lint:fix && npm run pretty:fix"
    },
    "author": "Andrei Auchynnikau",
    "keywords": [],
    "license": "ISC",
    "devDependencies": {
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.18.0",
        "eslint-config-airbnb": "^19.0.4",
        "jest": "^28.1.1",
        "nodemon": "^2.0.16",
        "prettier": "^2.7.1",
        "ts-node": "^10.8.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "jimp": "^0.16.1",
        "robotjs": "^0.6.0",
        "ws": "^8.8.0"
    }
}
